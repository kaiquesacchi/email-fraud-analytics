{
  "name": "email-fraud-analytics",
  "version": "0.1.0",
  "main": "public/electron.js",
  "author": "Kaique Sacchi",
  "build": {
    "appId": "com.kaiquesacchi.email-fraud-analytics"
  },
  "homepage": "./",
